<HBox fx:id="hBoxEachItem" prefHeight="41.0" prefWidth="626.0" spacing="10" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gauravdahal.ais.r.initial.RecruitItemController">
   <children>
      <Label fx:id="labelName" alignment="TOP_LEFT" prefHeight="17.0" prefWidth="129.0" text="Ram Gopal Verma">
